(define cons-pair (cons 1 2))  ;creates a pair (1 . 2)

(define list-3-nums (cons 1 (cons 2 (cons 3 '()))))  ;creates the list (1 2 3)

(define mixed-list-cons (cons "exampleString" (cons 1 (cons (cons 2 (cons 3 (cons 4 '()))) '()))))  ;creates ("example" 42 (1 2 3))

(define mixed-list-list (list "exampleString" 1 (list 2 3 4)))  ;creates ("example" 42 (1 2 3))

(define mixed-list-append (append (list "exampleString" 1) (list (list 2 3 4))))  ;creates ("example" 42 (1 2 3))

;;peinting out each structure
(display "cons-pair: ")
(display cons-pair)
(newline)

(display "list-3-nums: ")
(display list-3-nums)
(newline)

(display "mixed-list-cons: ")
(display mixed-list-cons)
(newline)

(display "mixed-list-list: ")
(display mixed-list-list)
(newline)

(display "mixed-list-append: ")
(display mixed-list-append)
(newline)